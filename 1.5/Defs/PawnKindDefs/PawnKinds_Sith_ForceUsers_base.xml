	<PawnKindDef Name="KotORSith_BASEdarkjedi" ParentName="KotORSith_ForceUserXenoList" Abstract="True">
		<defName>KotORSith_BASEdarkjedi</defName>
		<label>dark Jedi</label>
		<combatPower>200</combatPower>
		<minGenerationAge>25</minGenerationAge>
		<maxGenerationAge>45</maxGenerationAge>
		<itemQuality>Good</itemQuality>
		<!--backstoryFiltersOverride Inherit="False">
			  <li>
				<categories>
				  <li>Sith</li>
				</categories>
			  </li>
		  </backstoryFiltersOverride-->
		<apparelMoney>1000~5000</apparelMoney>
		<apparelTags>
			<li>KotORShield_sith</li>
			<li>KotOREquipment_sith</li>
			<li>Cape</li>
			<li>KotORHeadband_jedi</li>
			<li>SE_sithmask</li>
		</apparelTags>
		<apparelRequired>
			<li>guy762_JediTunic_formal</li>
		</apparelRequired>
		<ignoreFactionApparelStuffRequirements>true</ignoreFactionApparelStuffRequirements>
		<specificApparelRequirements>
			<li>
				<apparelLayer>OnSkin</apparelLayer>
				<color>(65,60,60)</color>
			</li>
			<li>
				<apparelLayer>Shell</apparelLayer>
				<stuff>Synthread</stuff>
				<color>(75,75,75)</color>
			</li>
		</specificApparelRequirements>

		<weaponMoney>99999</weaponMoney>
		<weaponTags>
			<li>SaV_darkjedisaber</li>
		</weaponTags>
		<biocodeWeaponChance>999</biocodeWeaponChance>

		<skills>
			<li>
				<skill>Melee</skill>
				<range>12~16</range>
			</li>
		</skills>

		<initialWillRange>10~12</initialWillRange>
		<initialResistanceRange>50~72</initialResistanceRange>

	</PawnKindDef>


	<PawnKindDef Name="KotORSith_BASEredguard" ParentName="KotORSith_TrooperBase" Abstract="True">
		<defName>KotORSith_redguard</defName>
		<label>Sith sovereign protector</label>
		<combatPower>300</combatPower>
		<maxPerGroup>6</maxPerGroup>
		<minGenerationAge>25</minGenerationAge>
		<maxGenerationAge>45</maxGenerationAge>

		<isBoss>true</isBoss>
		<skipResistant>true</skipResistant>

		<itemQuality>Excellent</itemQuality>
		<apparelMoney>999999</apparelMoney>
		<apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
		<apparelTags>
			<li>KotORUtility_stronggrenade</li>
			<li>KotOREquipment_sith</li>
		</apparelTags>
		<apparelRequired>
			<li>Apparel_Cape</li>
			<li>guy762_SithHelmet_colorcommando</li>
			<li>guy762_SithArmor_colorcommando</li>
			<li>guy762_Bodysuit</li>
			<li>guy762_energyshield_echaniHvy</li>
		</apparelRequired>
		<ignoreFactionApparelStuffRequirements>true</ignoreFactionApparelStuffRequirements>
		<specificApparelRequirements>
			<li>
				<apparelLayer>OnSkin</apparelLayer>
				<color>(100,100,100)</color>
			</li>
			<li>
				<apparelLayer>Overhead</apparelLayer>
				<color>(120,0,0)</color>
			</li>
			<li>
				<apparelLayer>Middle</apparelLayer>
				<color>(120,0,0)</color>
			</li>
			<li>
				<apparelLayer>Shell</apparelLayer>
				<stuff>Hyperweave</stuff>
				<color>(140,0,0)</color>
			</li>
		</specificApparelRequirements>

		<biocodeWeaponChance>0.95</biocodeWeaponChance>
		<!--weaponMoney>15000~100000</weaponMoney>
		<weaponTags>
		  <li>SE_redguard</li>
		  <li>SE_redguardmelee</li> moved to spawnable PotF pawns document and split between two pawnkinds because this refuses to generate pawns with ranged weapons 
		</weaponTags-->

		<techHediffsMoney>999999</techHediffsMoney>
		<techHediffsChance>100</techHediffsChance>
		<techHediffsRequired>
			<li>guy762_ImplantDock_II</li>
		</techHediffsRequired>

		<skills>
			<li>
				<skill>Shooting</skill>
				<range>16~18</range>
			</li>
			<li>
				<skill>Melee</skill>
				<range>16~18</range>
			</li>
		</skills>
		<inventoryOptions>
			<skipChance>0</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>guy762_stim_echani</thingDef>
					<countRange>1~2</countRange>
				</li>
				<li>
					<thingDef>guy762_stim_hyperdex</thingDef>
					<countRange>1~2</countRange>
				</li>
				<li>
					<thingDef>guy762_medpac_life</thingDef>
					<countRange>2~3</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>

		<disallowedTraits Inherit="False">
			<li>BodyPurist</li>
			<li>Wimp</li>
			<li>Brawler</li>
			<li>PsychicSensitivity</li>
		</disallowedTraits>
		<disallowedTraitsWithDegree Inherit="False" />
		<initialWillRange>0.5~2</initialWillRange>
		<initialResistanceRange>5~10</initialResistanceRange>
		<modExtensions Inherit="False">
			<li Class="BodyTypeForPawnKind.ModExtension_BodyTypesOfPawnKind">
				<whitelist>
				</whitelist>
				<blacklist>
					<li>Thin</li>
					<li>Fat</li>
					<li>Baby</li>
					<li>Child</li>
				</blacklist>
				<priority>
					<!--When violates the black/whitelist, selected from these-->
				</priority>
				<!--Align body gender to pawn's gender (default: True)-->
				<checkGender>True</checkGender>
			</li>
		</modExtensions>
	</PawnKindDef>




