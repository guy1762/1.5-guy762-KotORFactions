	<PawnKindDef Name="KotORRepub_jediguardian_PotF" ParentName="KotORRepub_BASEjediguardian">
      <defName>KotORRepub_jediguardian_PotF</defName>
	  <modExtensions>
		 <li Class="AddGeneForPK.PKAdditionalGenes">
		  <geneSets>
			<li>
			  <isXenogene>true</isXenogene>
			  <chance>1</chance>
			  <geneDefs>
				<li>SWPotF_Gene_LSEnlightenmentII</li>
			  </geneDefs>
			</li>
		  </geneSets>
		 </li>
		 <li Class="ItsSorceryFramework.ModExtension_PawnGenSchemaSet">
				<schemaSets>
				  <li>
					<schemaMappings> <!-- \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\ -->
					  
					  <li>
						<schema>SWPotF_ForceSchema</schema> <!-- mobility & telekinetics -->
						<weight>1</weight>
						
						<forceLevel>True</forceLevel>
						<level>15</level>
						<baseClassDef>SWPotF_ProgressTrackerClass_1thru10_ForceLvl</baseClassDef>
						<classChanges>
							<li>
								<classDef>SWPotF_ProgressTrackerClass_10thru20_ForceLvl</classDef>
								<levelReset>False</levelReset>
								<benefitReset>False</benefitReset>
								<removePostClassChange>True</removePostClassChange>
							</li>
						</classChanges>
						
						<requiredNodes>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_push</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_pull</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_repulse</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_wave</nodeDef><forceLevel>True</forceLevel></li>
						  
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_speedI</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_speedII</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_speedIII</nodeDef><forceLevel>True</forceLevel></li>
						  
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_jumpI</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_jumpII</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeUniv_jumpIII</nodeDef><forceLevel>True</forceLevel></li>
						  
						  <li><nodeDef>SWPotF_ForceTreeNodeLS_cure</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeLS_heal</nodeDef><forceLevel>True</forceLevel></li>
						  
						  <li><nodeDef>SWPotF_ForceTreeNodeLS_valorI</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeLS_valorII</nodeDef><forceLevel>True</forceLevel></li>
						  <li><nodeDef>SWPotF_ForceTreeNodeLS_valorIII</nodeDef><forceLevel>True</forceLevel></li>
						  
						  <!-- make another centered around disarm and stun -->
						  
						</requiredNodes>
					  </li>
					  
					  
					</schemaMappings> <!-- \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\ -->
				  </li>
				  
				</schemaSets>
		  </li>
      </modExtensions>
   </PawnKindDef>
   
   
   
   
   
   
